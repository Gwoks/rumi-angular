# RUMI Angular Application Environment Variables
# Copy this file to .env and update the values

# API Configuration
NG_APP_API_URL=http://localhost:3000
NG_APP_API_VERSION=v1

# Application Info
NG_APP_NAME=RUMI
NG_APP_VERSION=1.0.0
NG_APP_ENVIRONMENT=development

# Feature Flags
NG_APP_ENABLE_ANALYTICS=false
NG_APP_ENABLE_DEBUG=true
NG_APP_ENABLE_NOTIFICATIONS=true
NG_APP_ENABLE_DARK_MODE=false

# File Upload
NG_APP_MAX_FILE_SIZE=5MB